<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startFragment">
    <fragment
        android:id="@+id/startFragment"
        android:name="com.hifit.android.mafit.ui.fragment.start.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_startFragment_to_surveyStartFragment"
            app:destination="@id/surveyStartFragment" />
        <action
            android:id="@+id/action_startFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.hifit.android.mafit.ui.fragment.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_surveyStartFragment"
            app:destination="@id/surveyStartFragment"
            app:popUpTo="@id/startFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_kakaoLoginWebFragment"
            app:destination="@id/kakaoLoginWebFragment" />
        <argument
            android:name="code"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/surveyStartFragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStartFragment"
        android:label="fragment_survey_start"
        tools:layout="@layout/fragment_survey_start" >
        <action
            android:id="@+id/action_surveyStartFragment_to_surveyStep1Fragment"
            app:destination="@id/surveyStep1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep1Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep1Fragment"
        android:label="fragment_survey_step1"
        tools:layout="@layout/fragment_survey_step1" >
        <action
            android:id="@+id/action_surveyStep1Fragment_to_surveyStep2Fragment"
            app:destination="@id/surveyStep2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep2Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep2Fragment"
        android:label="fragment_survey_step2"
        tools:layout="@layout/fragment_survey_step2" >
        <action
            android:id="@+id/action_surveyStep2Fragment_to_surveyStep3Fragment"
            app:destination="@id/surveyStep3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep3Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep3Fragment"
        android:label="fragment_survey_step3"
        tools:layout="@layout/fragment_survey_step3" >
        <action
            android:id="@+id/action_surveyStep3Fragment_to_surveyStep4Fragment"
            app:destination="@id/surveyStep4Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep4Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep4Fragment"
        android:label="fragment_survey_step4"
        tools:layout="@layout/fragment_survey_step4" >
        <action
            android:id="@+id/action_surveyStep4Fragment_to_surveyStep5Fragment"
            app:destination="@id/surveyStep5Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep5Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep5Fragment"
        android:label="fragment_survey_step5"
        tools:layout="@layout/fragment_survey_step5" >
        <action
            android:id="@+id/action_surveyStep5Fragment_to_surveyStep6Fragment"
            app:destination="@id/surveyStep6Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep6Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep6Fragment"
        android:label="fragment_survey_step6"
        tools:layout="@layout/fragment_survey_step6" >
        <action
            android:id="@+id/action_surveyStep6Fragment_to_surveyStep7Fragment"
            app:destination="@id/surveyStep7Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep7Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep7Fragment"
        android:label="fragment_survey_step7"
        tools:layout="@layout/fragment_survey_step7" >
        <action
            android:id="@+id/action_surveyStep7Fragment_to_surveyStep8Fragment"
            app:destination="@id/surveyStep8Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep8Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep8Fragment"
        android:label="fragment_survey_step8"
        tools:layout="@layout/fragment_survey_step8" >
        <action
            android:id="@+id/action_surveyStep8Fragment_to_surveyStep9Fragment"
            app:destination="@id/surveyStep9Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep9Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep9Fragment"
        android:label="fragment_survey_step9"
        tools:layout="@layout/fragment_survey_step9" >
        <action
            android:id="@+id/action_surveyStep9Fragment_to_surveyStep10Fragment"
            app:destination="@id/surveyStep10Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep10Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep10Fragment"
        android:label="fragment_survey_step10"
        tools:layout="@layout/fragment_survey_step10" >
        <action
            android:id="@+id/action_surveyStep10Fragment_to_surveyStep11Fragment"
            app:destination="@id/surveyStep11Fragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyStep11Fragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyStep11Fragment"
        android:label="fragment_survey_step11"
        tools:layout="@layout/fragment_survey_step11" >
        <action
            android:id="@+id/action_surveyStep11Fragment_to_surveyCompleteFragment"
            app:destination="@id/surveyCompleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyCompleteFragment"
        android:name="com.hifit.android.mafit.ui.fragment.survey.SurveyCompleteFragment"
        android:label="fragment_survey_complete"
        tools:layout="@layout/fragment_survey_complete" >
        <action
            android:id="@+id/action_surveyCompleteFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.hifit.android.mafit.ui.fragment.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_surveyCompleteFragment"
            app:destination="@id/surveyCompleteFragment" />
        <action
            android:id="@+id/action_homeFragment_to_exerciseFragment"
            app:destination="@id/exerciseFragment" />
    </fragment>
    <fragment
        android:id="@+id/exerciseFragment"
        android:name="com.hifit.android.mafit.ui.fragment.exercise.ExerciseFragment"
        android:label="fragment_exercise"
        tools:layout="@layout/fragment_exercise" />
    <fragment
        android:id="@+id/kakaoLoginWebFragment"
        android:name="com.hifit.android.mafit.ui.fragment.login.KakaoLoginWebFragment"
        android:label="KakaoLoginWebFragment" >
        <action
            android:id="@+id/action_kakaoLoginWebFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>